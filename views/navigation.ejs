<script>
  var ajax = new XMLHttpRequest();
  ajax.onreadystatechange = function () {
    if (ajax.readyState == 4 && ajax.status == 200) {
      document.getElementById("modal-body").innerHTML = ajax.responseText;
    }
  };
  ajax.open("GET", "/project/new", true);
  ajax.send();
</script>

